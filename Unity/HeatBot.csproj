      <LastGenOutput>mineur.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="monster.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>monster.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="paysan.resx">
