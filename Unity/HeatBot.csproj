    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="TreasureHunter.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>TreasureHunter.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="alchimiste.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>alchimiste.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="bucheron.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>bucheron.Designer.cs</LastGenOutput>
